{
  "name": "weather-today",
  "description": "Weather Today",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --fix",
    "prettier": "prettier --ignore-unknown --no-error-on-unmatched-pattern --write \"**/*.{js,jsx,ts,tsx,md,xml,svg}\"",
    "type": "tsc --noEmit --incremental",
    "format": "pnpm run prettier && pnpm run lint && pnpm run type",
    "bootstrap": "pnpm install && pnpm run format",
    "tsc:clean": "tsc --build --clean",
    "svgr": "svgr -d components/icons --icon --typescript icons --no-prettier && prettier components/icons --write"
  },
  "dependencies": {
    "axios": "1.7.7",
    "clsx": "2.1.1",
    "date-fns": "4.1.0",
    "jotai": "2.10.0",
    "next": "14.2.13",
    "react": "18",
    "react-dom": "18",
    "react-icons": "5.3.0",
    "react-query": "3.39.3",
    "tailwind-merge": "2.5.2"
  },
  "devDependencies": {
    "@types/node": "22",
    "@types/react": "18",
    "@types/react-dom": "18",
    "@next/bundle-analyzer": "14.2.13",
    "@svgr/cli": "8.1.0",
    "@svgr/webpack": "8.1.0",
    "autoprefixer": "10.4.20",
    "eslint": "8.57.1",
    "prettier": "3.3.3",
    "eslint-config-next": "14.2.13",
    "postcss": "8",
    "tailwindcss": "3.4.12",
    "typescript": "5"
  }
}
